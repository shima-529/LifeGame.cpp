class Cell {
	private:
		bool status;
		bool nextStatus;

	public:
		Cell();
		static int N;
		static int repeatNum;
		static int delayTime;
		static int dataSet;
		static bool shouldClearScreen;
		static bool shouldUseColor;

		bool getStatus();
		bool getNextStatus();
		void setStatus(bool param);
		void setNextStatus(bool p);

		void goToNextStep();
		bool compareOwnStatus();
};
